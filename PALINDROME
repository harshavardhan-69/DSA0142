#include<iostream>
using namespace std;
int main()
{
	int n,r,sum=0,temp;
	cout<<"enter :";
	cin>>n;
	temp=n;
	while(n>0)
	{
		r =n%10;
        sum=(sum*10)+r;
        n = n / 10;
    }
    if(temp == sum)
    cout << " palindrome" << endl;
    else
    cout << "Not palindrome" << endl;
    return 0;
}

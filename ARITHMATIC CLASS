#include<iostream>
#include<conio.h>
 using namespace std;
  class arithmetic
   {
    private:
       int a,b;
    public:
      void getdata()
       {
         cout<<"enter two number:-";
         cin>>a>>b;
       }
     void Addition()
       {
        int c=a+b;
         cout<<"\nAddition = "<<c;
       }
     void Subtraction()
      {
        int c=a-b;
         cout<<"\nSubtraction = "<<c;
      }
     void Multiplication()
      {
        int c=a*b;
         cout<<"\nMultiplication = "<<c;
      }
       void Division()
      {
        int c=a/b;
         cout<<"\nDivision = "<<c;
      }
    };
  int main()
   {
     arithmetic a1;
     a1.getdata(); 
     a1.Addition();
     a1.Division();
     a1.Subtraction();
     a1.Multiplication();    
     getch();
   }
